
@using Quanlysach.Models
@model IEnumerable<CHUDE>
<div class="title">Chủ đề sách</div>
<ul class="list-group">
    @foreach (var chude in Model)
    {
        <a href="@Url.Action("SPTheoChuDe","Bookstore",new {id=chude.MaCD})" class="list-group-item list-group-item-action " >
            @chude.TenChuDe
        </a>
    }
</ul>